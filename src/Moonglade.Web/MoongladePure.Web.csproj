      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Moonglade.Auth\MoongladePure.Auth.csproj" />
    <ProjectReference Include="..\Moonglade.Comments\MoongladePure.Comments.csproj" />
